{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/client",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/backoffice/assets/vendors/mdi/css/materialdesignicons.min.css",
              "src/assets/backoffice/assets/vendors/css/vendor.bundle.base.css",
              "src/assets/backoffice/assets/vendors/jvectormap/jquery-jvectormap.css",
              "src/assets/backoffice/assets/vendors/owl-carousel-2/owl.carousel.min.css",
              "src/assets/backoffice/assets/vendors/owl-carousel-2/owl.theme.default.min.css",
              "src/assets/backoffice/assets/css/style.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/backoffice/assets/vendors/js/vendor.bundle.base.js",
              "src/assets/backoffice/assets/vendors/chart.js/Chart.min.js",
              "src/assets/backoffice/assets/vendors/progressbar.js/progressbar.min.js",
              "src/assets/backoffice/assets/vendors/jvectormap/jquery-jvectormap.min.js",
              "src/assets/backoffice/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js",
              "src/assets/backoffice/assets/vendors/owl-carousel-2/owl.carousel.min.js",
              "src/assets/backoffice/assets/js/jquery.cookie.js",
              "src/assets/backoffice/assets/js/off-canvas.js",
              "src/assets/backoffice/assets/js/hoverable-collapse.js",
              "src/assets/backoffice/assets/js/misc.js",
              "src/assets/backoffice/assets/js/settings.js",
              "src/assets/backoffice/assets/js/todolist.js",
              "src/assets/backoffice/assets/js/dashboard.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "client:build:production"
            },
            "development": {
              "buildTarget": "client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
